{
  "name": "@rmkane/key-event-to-string",
  "private": true,
  "version": "0.1.0",
  "description": " Converts a JavaScript key event object into a humanly readable format",
  "author": "Ryan Kane",
  "license": "MIT",
  "type": "module",
  "main": "dist/key-event-to-string.cjs.js",
  "module": "dist/key-event-to-string.es.js",
  "browser": "dist/key-event-to-string.iife.js",
  "types": "dist/index.d.ts",
  "file": [
    "dist",
    "lib",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmkane/key-event-to-string.git"
  },
  "keywords": [
    "events",
    "keyboard"
  ],
  "homepage": "https://github.com/rmkane/key-event-to-string#readme",
  "bugs": {
    "url": "https://github.com/rmkane/key-event-to-string/issues"
  },
  "scripts": {
    "dev": "vite --config vite.config.js",
    "clean": "rm -rf dist docs lib",
    "build": "npm run clean && npm run build:all",
    "build:all": "npm run build:dist && npm run build:docs && npm run build-demo",
    "build:docs": "typedoc src/index.ts",
    "build:lib": "tsc",
    "build:dist": "npm run build:lib && vite build",
    "build-demo": "cd demo && npm run build",
    "lint": "eslint --ignore-path .eslintignore --ext .ts ./src",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2"
  }
}
